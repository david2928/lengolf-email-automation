# Project Configuration
PROJECT_ID=your_project_id
REGION=your_region

# Google Sheets IDs
FACEBOOK_SHEET_ID=your_facebook_sheet_id
FACEBOOK_B2B_SHEET_ID=your_b2b_sheet_id

# Gmail Labels
LABEL_CLASSPASS=Web Leads (ClassPass)
LABEL_WEB=Web Leads
LABEL_RESOS=Web Leads (ResOS)
LABEL_FACEBOOK=Web Leads (Facebook)
LABEL_COMPLETED=Web Leads - completed

# Facebook/Meta Configuration
META_ACCESS_TOKEN=your_meta_access_token
META_PAGE_ID=your_meta_page_id
FORM_ID_B2B_NEW=your_b2b_form_id
FORM_ID_B2C_NEW=your_b2c_form_id

# Supabase Configuration
SUPABASE_URL=your_supabase_url
SUPABASE_SERVICE_ROLE_KEY=your_supabase_key

# Google Cloud and Vertex AI Configuration
VERTEX_LOCATION=us-central1
ENABLE_LLM_SPAM_DETECTION=true
GOOGLE_APPLICATION_CREDENTIALS=path/to/credentials.json

# LINE Messaging API Configuration
# You only need ONE channel access token for ALL groups
LINE_CHANNEL_ACCESS_TOKEN=your_line_channel_access_token
LINE_CHANNEL_SECRET=your_line_channel_secret

# LINE Group IDs for different notification types
LINE_GROUP_ID=your_default_group_id  # Default fallback group
LINE_GROUP_ID_B2B=your_b2b_group_id
LINE_GROUP_ID_B2C=your_b2c_group_id
LINE_GROUP_ID_CLASSPASS=your_classpass_group_id
LINE_GROUP_ID_WEBRESOS=your_webresos_group_id

